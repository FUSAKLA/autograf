{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Autograf",
  "id": "fusaklacz-autograf-app",
  "info": {
    "keywords": [
      "app"
    ],
    "description": "Automatically generate dashboards from prometheus metrics",
    "author": {
      "name": "Fusakla cz"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Generate dashboard",
      "path": "/a/%PLUGIN_ID%/generate",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.0.3",
    "plugins": []
  }
}
